{
    "problemMatcher": [
        {
            "owner": "spotter",
            "pattern": [
                {
                    "regexp": "^([a-zA-Z0-9-\\.\\/]+):(\\d+)\\s+([a-zA-Z]+)\\s+(.+)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "severity": 4,
                    "code": 5,
                    "message": 6
                }
            ]
        }
    ]
}
