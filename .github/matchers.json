{
    "problemMatcher": [
        {
            "owner": "spotter",
            "pattern": [
                {
                    "regexp": "^^([a-zA-Z0-9-\\.\\/_]+):(\\d+):(\\d+):\\s+([a-zA-Z]+):\\s+\\[([a-zA-Z0-9]+)\\]\\s+(.+)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "severity": 4,
                    "code": 5,
                    "message": 6
                }
            ]
        }
    ]
}
